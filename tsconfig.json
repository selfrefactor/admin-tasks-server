{
  "compilerOptions": {
    "resolveJsonModule": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "esNext",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "paths": {
      "lib/ngrok": [
        "libs/ngrok/src"
      ],
      "lib/ngrok/*": [
        "libs/ngrok/src/*"
      ],
      "lib/constants": [
        "libs/constants/src"
      ],
      "lib/constants/*": [
        "libs/constants/src/*"
      ],
      "lib/speed-reader": [
        "libs/speed-reader/src"
      ],
      "lib/speed-reader/*": [
        "libs/speed-reader/src/*"
      ],
      "lib/fs": [
        "libs/fs/src"
      ],
      "lib/fs/*": [
        "libs/fs/src/*"
      ],
      "lib/word-profile": [
        "libs/word-profile/src"
      ],
      "lib/word-profile/*": [
        "libs/word-profile/src/*"
      ],
      "lib/db-fs": [
        "libs/db-fs/src"
      ],
      "lib/db-fs/*": [
        "libs/db-fs/src/*"
      ],
      "lib/utils": [
        "libs/utils/"
      ],
      "lib/utils/*": [
        "libs/utils/*"
      ],
      "lib/rss-translate": [
        "libs/rss-translate/src"
      ],
      "lib/rss-translate/*": [
        "libs/rss-translate/src/*"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}
