{
	"name": "on",
	"version": "1.0.0",
	"description": "Turn server ON",
	"author": "selfrefactor",
	"license": "MIT",
	"scripts": {
		"update": "nest update",
		"middleware": "nest g middleware",
		"library": "nest g library",
		"controller": "nest g controller --no-spec",
		"service": "nest g service --no-spec",
		"module": "nest g module --no-spec",
		"preout": "rimraf dist",
		"predev": "rimraf dist",
		"prein": "rimraf dist",
		"prelight": "yarn out",
		"preonclean": "yarn out",
		"preondarkclean": "yarn out",
		"out": "nest build",
		"niketa": "maided niketa",
		"on": "maided on",
		"onclean": "maided on",
		"ondark": "maided on:dark",
		"ondarkclean": "maided on:dark",
		"light": "maided server:light",
		"dark": "maided server:dark",
		"prod": "node dist/main.js",
		"in": "DEV_MODE=ON MONGO=ON PORT=8080 nest start --watch",
		"dev": "DEV_MODE=ON PORT=8070 nest start --debug --watch",
		"debug": "DEV_MODE=ON PORT=8080 nest start --debug --watch",
		"test": "jest",
		"test:ts": "typescript-coverage-report --threshold=40",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "jest --config ./test/jest-e2e.json",
		"e2e": "jest --config ./test/jest-e2e.json"
	},
	"dependencies": {
		"@nestjs/common": "7.0.9",
		"@nestjs/config": "0.4.0",
		"@nestjs/core": "7.0.9",
		"@nestjs/graphql": "7.3.5",
		"@nestjs/mongoose": "6.4.0",
		"@nestjs/platform-express": "7.0.9",
		"apollo-server-express": "2.13.0-alpha.1",
		"capture-error-fn": "0.1.0",
		"class-transformer": "0.2.3",
		"class-validator": "^0.12.2",
		"db-fn": "3.0.0",
		"env-fn": "2.1.0",
		"fs-extra": "9.0.0",
		"graphql": "15.0.0",
		"graphql-tools": "5.0.1-ffb435a.0",
		"helpers-fn": "0.2.0",
		"mongodb": "3.6.0-beta.0",
		"mongoose": "5.9.11",
		"ngrok": "3.2.7",
		"rabbit-fn": "1.2.1",
		"rambdax": "3.7.0",
		"reflect-metadata": "0.1.13",
		"rxjs": "7.0.0-beta.0",
		"string-fn": "2.11.1",
		"sudo-prompt": "9.2.1",
		"type-graphql": "0.18.0-beta.17"
	},
	"devDependencies": {
		"@nestjs/cli": "7.1.4",
		"@nestjs/schematics": "7.0.0",
		"@nestjs/testing": "7.0.9",
		"@types/axios": "0.14.0",
		"@types/express": "4.17.6",
		"@types/fs-extra": "8.1.0",
		"@types/jest": "25.2.1",
		"@types/node": "13.13.4",
		"axios": "0.19.2",
		"jest": "^24.9.0",
		"maided": "0.1.3",
		"rimraf": "3.0.2",
		"ts-jest": "25.10.0-alpha.2",
		"ts-loader": "7.0.2",
		"ts-node": "8.9.1",
		"tsconfig-paths": "3.9.0",
		"tslint-fn": "0.2.2",
		"type-coverage": "2.4.3",
		"typescript": "^3.8.3",
		"typescript-coverage-report": "0.1.3"
	},
	"depFn": [
		"typeorm",
		"typescript",
		"jest"
	],
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": ".",
		"testRegex": ".spec.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"coverageDirectory": "./coverage",
		"testEnvironment": "node",
		"roots": [
			"<rootDir>/src/",
			"<rootDir>/libs/"
		],
		"moduleNameMapper": {
			"lib/ngrok/(.*)": "<rootDir>/libs/ngrok/src/$1",
			"lib/ngrok": "<rootDir>/libs/ngrok/src",
			"lib/constants/(.*)": "<rootDir>/libs/constants/src/$1",
			"lib/constants": "<rootDir>/libs/constants/src",
			"lib/speed-reader/(.*)": "<rootDir>/libs/speed-reader/src/$1",
			"lib/speed-reader": "<rootDir>/libs/speed-reader/src",
			"lib/fs/(.*)": "<rootDir>/libs/fs/src/$1",
			"lib/fs": "<rootDir>/libs/fs/src",
			"lib/word-profile/(.*)": "<rootDir>/libs/word-profile/src/$1",
			"lib/word-profile": "<rootDir>/libs/word-profile/src",
			"lib/db-fs/(.*)": "<rootDir>/libs/db-fs/src/$1",
			"lib/db-fs": "<rootDir>/libs/db-fs/src",
			"lib/utils/(.*)": "<rootDir>/libs/utils/$1",
			"lib/utils": "<rootDir>/libs/utils/"
		}
	},
	"commitMessage": {
		"workInProgress": "",
		"support": [
			"maid",
			"codesignal"
		],
		"feature": [
			"lib",
			"route",
			"util"
		]
	}
}
