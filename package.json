{
  "name": "on",
  "version": "1.0.0",
  "description": "Turn server ON",
  "author": "selfrefactor",
  "license": "MIT",
  "scripts": {
    "middle": "nest g middleware",
    "middleware": "nest g middleware",
    "lib": "nest g library",
    "library": "nest g library",
    "control": "nest g controller --no-spec",
    "controller": "nest g controller --no-spec",
    "service": "nest g service --no-spec",
    "module": "nest g module --no-spec",
    "preout": "rimraf dist",
    "predev": "rimraf dist",
    "prein": "rimraf dist",
    "preon": "yarn out",
    "preondark": "yarn out",
    "out": "nest build",
    "lint": "maided lint",
    "niketa": "maided niketa",
    "niketa:simple": "maided niketa:simple",
    "on": "maided on",
    "ondark": "maided on:dark",
    "prod": "node dist/main.js",
    "in": "DEV_MODE=ON PORT=8080 nest start --watch",
    "dev": "DEV_MODE=ON PORT=8070 nest start --debug --watch",
    "debug": "DEV_MODE=ON PORT=8080 nest start --debug --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cover": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^6.10.14",
    "@nestjs/config": "^0.4.0",
    "@nestjs/core": "^6.10.14",
    "@nestjs/platform-express": "^6.10.14",
    "@nestjs/typeorm": "^7.0.0",
    "axios": "^0.19.2",
    "capture-error": "https://github.com/selfrefactor/capture-error#1.4.0",
    "client-helpers": "https://github.com/selfrefactor/client-helpers#2.0.0",
    "dayjs": "^1.8.19",
    "db-fn": "^3.0.0",
    "env-fn": "2.1.0",
    "execa": "^4.0.0",
    "fs-extra": "^8.1.0",
    "helpers": "https://github.com/selfrefactor/helpers#2.4.0",
    "maided": "0.1.1",
    "mongodb": "^3.5.5",
    "ngrok": "3.2.7",
    "rabbit-fn": "^1.2.0",
    "rambdax": "^3.6.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.4",
    "string-fn": "^2.10.2",
    "typeorm": "^0.2.24"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.14.2",
    "@nestjs/schematics": "^6.8.1",
    "@nestjs/testing": "^6.10.14",
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.2",
    "@types/jest": "25.1.2",
    "@types/node": "^13.1.6",
    "@types/supertest": "^2.0.8",
    "jest": "^24.9.0",
    "jest-extended": "0.11.5",
    "supertest": "^4.0.2",
    "ts-jest": "25.2.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.7.0",
    "tsconfig-paths": "^3.9.0",
    "tslint-fn": "0.2.1",
    "typescript": "^3.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "lib/ngrok/(.*)": "<rootDir>/libs/ngrok/src/$1",
      "lib/ngrok": "<rootDir>/libs/ngrok/src",
      "lib/constants/(.*)": "<rootDir>/libs/constants/src/$1",
      "lib/constants": "<rootDir>/libs/constants/src",
      "lib/speed-reader/(.*)": "<rootDir>/libs/speed-reader/src/$1",
      "lib/speed-reader": "<rootDir>/libs/speed-reader/src",
      "lib/fs/(.*)": "<rootDir>/libs/fs/src/$1",
      "lib/fs": "<rootDir>/libs/fs/src",
      "lib/fs-db/(.*)": "<rootDir>/libs/fs-db/src/$1",
      "lib/fs-db": "<rootDir>/libs/fs-db/src"
    }
  },
  "commitMessage": {
    "workInProgress": "",
    "support": [
      "maid",
      "codesignal"
    ],
    "feature": [
      "lib"
    ]
  }
}
