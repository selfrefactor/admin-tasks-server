{
  "name": "on",
  "version": "1.0.0",
  "description": "Turn server ON",
  "author": "selfrefactor",
  "license": "MIT",
  "scripts": {
    "preout": "rimraf dist",
    "predev": "rimraf dist",
    "prein": "rimraf dist",
    "preon": "yarn out",
    "out": "nest build",
    "lint": "maid lint",
    "niketa": "maid niketa",
    "on": "maid on",
    "prod": "node dist/main.js",
    "on:l": "maid on",
    "on:d": "maid on:dark",
    "in": "DEV_MODE=ON PORT=8080 nest start --watch",
    "dev": "DEV_MODE=ON PORT=8080 nest start --debug --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cover": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^6.10.14",
    "@nestjs/core": "^6.10.14",
    "@nestjs/mongoose": "^6.3.1",
    "@nestjs/platform-express": "^6.10.14",
    "@nestjs/platform-fastify": "^6.11.8",
    "axios": "^0.19.2",
    "capture-error": "https://github.com/selfrefactor/capture-error#1.4.0",
    "client-helpers": "https://github.com/selfrefactor/client-helpers#2.0.0",
    "dayjs": "^1.8.19",
    "db-fn": "^1.0.0",
    "env-fn": "2.0.0",
    "execa": "^4.0.0",
    "fs-extra": "^8.1.0",
    "helpers": "https://github.com/selfrefactor/helpers#2.4.0",
    "mongoose": "^5.9.2",
    "rabbit-fn": "^1.2.0",
    "rambdax": "^3.6.0",
    "ngrok": "3.2.7",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.4",
    "string-fn": "^2.10.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.14.2",
    "@nestjs/schematics": "^6.8.1",
    "@nestjs/testing": "^6.10.14",
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.2",
    "@types/jest": "25.1.2",
    "@types/node": "^13.1.6",
    "@types/supertest": "^2.0.8",
    "esm": "^3.2.25",
    "jest": "^24.9.0",
    "maid": "^0.3.0",
    "supertest": "^4.0.2",
    "ts-jest": "25.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.0.0",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "lib/ngrok/(.*)": "<rootDir>/libs/ngrok/src/$1",
      "lib/ngrok": "<rootDir>/libs/ngrok/src",
      "lib/constants/(.*)": "<rootDir>/libs/constants/src/$1",
      "lib/constants": "<rootDir>/libs/constants/src"
    }
  }
}