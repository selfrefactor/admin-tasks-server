{
	"name": "admin-tasks",
	"niketaScripts": [
    {
      "**/*.ts": "yarn eslintx"
    },
    {
      "**/*.ts": "yarn eslintx"
    }
  ],
	"scripts": {
		"controller": "nest g controller --no-spec",
		"eslint": "eslint --fix src/app.controller.ts",
		"eslintx": "eslint --fix",
		"clean": "ts-prune -p tsconfig.json ",
		"teacher": "cd $HOME/repos/secret-services/packages/bot-teacher/server && node index.js",
		"x": "yarn teacher",
		"watch": "jest libs/foo.spec.ts --watch -u",
		"in": "DEV_MODE=ON nest start --watch",
		"dev": "DEV_MODE=ON PORT=8087 nest start --watch",
		"library": "nest g library",
		"middleware": "nest g middleware",
		"module": "nest g module --no-spec",
		"onx": "yarn on && yarn x",
		"out": "nest build",
		"predev": "rimraf dist",
		"prein": "rimraf dist",
		"preonclean": "yarn out",
		"preout": "rimraf dist",
		"on": "node dist/main.js",
		"onclean": "yarn on",
		"service": "nest g service",
		"type-coverage": "npx type-coverage",
		"test": "jest",
		"test:e2e": "jest --config ./test/jest-e2e.json -u",
		"test:e2e:watch": "jest  --watch --config ./test/jest-e2e.json -u",
		"test:ts": "typescript-coverage-report --threshold=40"
	},
	"version": "1.0.0",
	"dependencies": {
		"@nestjs/common": "10.3.0",
		"@nestjs/config": "3.1.1",
		"@nestjs/core": "10.3.0",
		"@nestjs/platform-express": "10.3.0",
		"@nestjs/serve-static": "4.0.0",
		"capture-error-fn": "0.1.0",
		"class-transformer": "0.5.1",
		"class-validator": "0.14.0",
		"dayjs": "1.11.10",
		"db-fn": "3.0.0",
		"env-fn": "2.2.0",
		"extract-zip": "2.0.1",
		"fs-extra": "11.2.0",
		"helpers-fn": "1.8.1",
		"rambdax": "10.1.0",
		"reflect-metadata": "0.2.1",
		"rxjs": "7.8.1",
		"string-fn": "3.1.0"
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "6.0.0",
    "@typescript-eslint/parser": "6.0.0",
    "eslint": "8.42.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.0",
    "prettier": "3.0.0",
		"@nestjs/cli": "10.2.1",
		"@nestjs/schematics": "10.0.3",
		"@nestjs/testing": "10.3.0",
		"@types/express": "4.17.21",
		"@types/fs-extra": "11.0.4",
		"@types/jest": "29.5.11",
		"@types/node": "20.10.5",
		"axios": "1.6.2",
		"jest": "29.7.0",
		"rimraf": "5.0.5",
		"ts-jest": "29.1.1",
		"ts-loader": "9.5.1",
		"ts-node": "10.9.2",
		"ts-prune": "0.10.3",
		"tsconfig-paths": "4.2.0",
		"typescript": "5.3.3"
	},
